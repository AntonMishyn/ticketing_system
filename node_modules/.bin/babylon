#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/anton/Projects/ticketing_system/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/babylon@6.18.0/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/node_modules:/home/anton/Projects/ticketing_system/node_modules:/home/anton/Projects/node_modules:/home/anton/node_modules:/home/node_modules:/node_modules:/home/anton/Projects/ticketing_system/node_modules/babylon/bin/node_modules:/home/anton/Projects/ticketing_system/node_modules/babylon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../babylon/bin/babylon.js" "$@"
else
  exec node  "$basedir/../babylon/bin/babylon.js" "$@"
fi
