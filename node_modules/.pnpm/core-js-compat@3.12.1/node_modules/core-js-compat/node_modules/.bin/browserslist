#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/anton/Projects/ticketing_system/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/browserslist@4.16.6/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/node_modules:/home/anton/Projects/ticketing_system/node_modules:/home/anton/Projects/node_modules:/home/anton/node_modules:/home/node_modules:/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/core-js-compat@3.12.1/node_modules/core-js-compat/node_modules:/home/anton/Projects/ticketing_system/node_modules/.pnpm/core-js-compat@3.12.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
fi
